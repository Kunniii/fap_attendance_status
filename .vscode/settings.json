{
  "editor.formatOnSave": true,
  "prettier.enable": true,
  "prettier.semi": true,
  "prettier.singleQuote": false,
  "tailwind-class-sorter.classRegex": {
    "rescript": [
      "className\\w*?=\\w*(\"[\\s\\S]+?\")|className\\w*?=\\w*?\\{([\\s\\S]+?)\\}",
      "\"(.+?)\""
    ]
  }
}
